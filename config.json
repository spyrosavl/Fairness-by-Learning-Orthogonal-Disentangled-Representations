{
    "name": "German",
    "n_gpu": 1,

    "arch": {
        "type": "TabularModel",
        "args": {
            "input_dim" : 61,
            "hidden_dim" : 30,
            "z_dim" : 2,
            "target_classes" : 1,
            "sensitive_classes" : 4 
        }
    },
    "data_loader": {
        "type": "GermanDataLoader",
        "args":{
            "data_dir": "data/",
            "batch_size": 128,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "nll_loss",
    "metrics": [
        "accuracy", "top_k_acc"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,

        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 10,

        "tensorboard": true,

        "lambda_e" : 1,
        "lambda_od" : 2,
        "gamma_e" : 1,
        "gamma_od" : 2,
        "step_size" : 1
 


    }
}
