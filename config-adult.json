{
    "name": "Adult",
    "n_gpu": 1,

    "arch": {
        "type": "TabularModel",
        "args": {
            "input_dim" : 108,
            "hidden_dim" : 64,
            "z_dim" : 2,
            "target_classes" : 1,
            "sensitive_classes" : 2
        }
    },
    "data_loader": {
        "type": "AdultDataLoader",
        "args":{
            "data_dir": "data/adult/",
            "batch_size": 64,
            "shuffle": true,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0.0005,
            "amsgrad": true
        }
    },
    "loss": "loss",
    "metrics": [
        "accuracy", "sens_accuracy"
    ],
    "trainer": {
        "epochs": 40,

        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 10,

        "tensorboard": true,

        "lambda_e" : 0.55,
        "lambda_od" : 0.037,
        "gamma_e" : 1.66,
        "gamma_od" : 0.8,
        "step_size" : 30
 


    }
}
